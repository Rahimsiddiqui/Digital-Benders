  <div class="newsletter-container">
    <h3 class="newsletter__title">Sign up to our <span>Newsletters</span></h3>

    <div id="spinner-overlay" style="z-index: 9999">
      <div
        class="spinner-border spinner text-white spinner-border-md"
        role="status"
      ></div>
    </div>

    <form class="newsletter__form" onsubmit="handleSubmit(event)">
      <div class="input-group">
        <label for="newsletter-email" class="seo-only"
          >Subscribe to our newsletter</label
        >
        <input
          class="form__input form-control"
          type="email"
          placeholder="Subscribe to our Newsletters"
          id="newsletter-email"
          required
        />
        <span class="input-group-text" id="addon-wrapping"
          ><button class="form__tele-icon">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              role="img"
              aria-label="Telegram Icon"
              width="23.568"
              height="23.568"
              viewBox="0 0 23.568 23.565"
            >
              <path
                id="Icon_ionic-ios-send"
                data-name="Icon ionic-ios-send"
                d="M21.386.049.274,9.251a.484.484,0,0,0,.017.88L6,13.357a.921.921,0,0,0,1.052-.1l11.26-9.707c.075-.063.253-.184.322-.115s-.04.247-.1.322L8.791,14.725a.918.918,0,0,0-.092,1.1l3.732,5.987a.486.486,0,0,0,.874-.011L22.036.688A.484.484,0,0,0,21.386.049Z"
                transform="matrix(0.998, 0.07, -0.07, 0.998, 1.54, 0)"
              ></path>
            </svg></button
        ></span>
      </div>
    </form>
  </div>
  <div class="footer__row">
    <div class="footer__payment-methods">
      <img
        src="https://www.creativesquad.ca/images/visa.svg"
        alt="We Accept Visa Payments"
        class="payment-method"
        loading="lazy"
      />
      <img
        src="https://www.creativesquad.ca/images/stripe.svg"
        alt="We Accept Stripe Payments"
        class="payment-method"
        loading="lazy"
      />
      <img
        src="https://www.creativesquad.ca/images/paypal.svg"
        alt="We Accept Paypal Payments"
        class="payment-method"
        loading="lazy"
      />
      <img
        src="https://www.creativesquad.ca/images/mastercard.svg"
        alt="We Accept Master Card Payments"
        class="payment-method"
        loading="lazy"
      />
      <img
        src="https://www.creativesquad.ca/images/pay.svg"
        alt="We Accept Amazon Pay Payments"
        class="payment-method"
        loading="lazy"
      />
    </div>
    <div class="watermark">
      &copy; <%= new Date().getFullYear() %> Digital Benders | All rights
      reserved
    </div>
  </div>
